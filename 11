#include <stdio.h>

int main()
{
    float num1 = 0.1f;           // float는 숫자 뒤에 f를 붙임
                                    
    double num2 = 3867.215820;     
                                  
    long double num3 = 9.327513l;    // long double은 숫자 뒤에 l을 붙임
                                      
    printf("%f %f %Lf\n", num1, num2, num3);    // 0.100000 3867.215820 9.327513

    return 0;
}
